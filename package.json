        "typescript": "~3.7.2",
        "yup": "^0.28.3"
    },
    "scripts": {
        "build:css": "node-sass src/styles/ -o src/styles/ ",
        "watch:css": "npm run build:css && node-sass src/styles/ -o src/styles/ -w -r",
        "start:js": "env-cmd -f ./config/localhost.env react-scripts start",
        "start": "npm-run-all -p watch:css start:js",
        "build:js": "env-cmd -f ./config/dev.env react-scripts build",
        "build": "npm-run-all build:*",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
