        "@types/node": "^12.0.0",
        "@types/node-sass": "^4.11.0",
        "@types/react": "^16.9.0",
        "@types/react-dom": "^16.9.0",
        "@types/react-dropzone": "^5.1.0",
        "@types/react-sortable-hoc": "^0.7.1",
        "@types/shortid": "^0.0.29",
        "@types/yup": "^0.26.35",
        "array-move": "^2.2.1",
        "env-cmd": "^10.1.0",
        "formik": "^2.1.4",
        "node-sass": "^4.13.1",
        "npm-run-all": "^4.1.5",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-dropzone": "^10.2.2",
        "react-scripts": "3.4.1",
